{
  "name": "argos-ui-product-test",
  "description": "Argos UI Products Test",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "--------- START ----------------------": "",
    "start": "yarn clean && cross-env NODE_ENV=development yarn development",
    "--------- DEVELOP ----------------------": "",
    "development": "concurrently --raw --kill-others \"npm:development:*\" \"yarn build:server --watch\"",
    "development:run:server": "wait-on ./dist/server.js --window 2000 && nodemon --watch dist/ ./dist/server.js",
    "development:run:dev-server": "cross-env NODE_ENV=development webpack-dev-server --hot --config ./config/webpack/webpack.config.js --mode development --port 3000",
    "development:run:stub": "nodemon --watch ./stubs ./scripts/start-stub.js",
    "--------- BUILD ----------------------": "",
    "build": "yarn clean && yarn build:client && yarn build:server --progress",
    "build:client": "webpack --progress --config ./config/webpack/webpack.config.js",
    "build:server": "webpack --config ./config/webpack/webpack.server.js",
    "--------- POSTINSTALL ----------------------": "",
    "postinstall": "yarn build",
    "--------- SERVER ----------------------": "",
    "serve": "node ./dist/server.js",
    "--------- TESTS ----------------------": "",
    "test": "jest --config ./config/jest/jest.config.js --watch",
    "--------- LINTING ----------------------": "",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:css": "stylelint --config config/stylelint/.stylelintrc \"./src/**/*.scss\"",
    "lint:js": "eslint --ext .js --ext .jsx src --config config/eslint/.eslintrc.js --ignore-path config/eslint/.eslintignore --fix",
    "fix:assets": "prettier --ignore-path .gitignore --write \"**/*.{json,md}\"",
    "--------- HELPERS ----------------------": "",
    "clean": "rimraf dist reports",
    "nuke": "yarn cache clean && yarn global add rimraf && rimraf node_modules && yarn && yarn run clean"
  },
